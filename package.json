{
  "name": "webclgl",
  "version": "4.0.22",
  "description": "GPGPU Javascript library",
  "repository": {
    "type": "git",
    "url": "https://github.com/stormcolor/webclgl.git"
  },
  "dependencies": {
  },
  "devDependencies": {
    "babel-plugin-syntax-async-functions": "~6.13.0",
    "babel-plugin-transform-regenerator": "~6.26.0",
    "babel-preset-es2015": "~6.24.1",
    "babelify": "~7.3.0",
    "eslint": "~4.11.0",
    "eslint-config-google": "~0.9.1",
    "mkdirp": "~0.5.1",
    "uglify-js": "~3.0.28",
    "watchify": "~3.9.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "syntax-async-functions",
            "transform-regenerator"
          ]
        }
      ]
    ]
  },
  "main": "src/webclgl/index.js",
  "scripts": {
    "build_&_watch": "node build.js -b -s",
    "build_&_deploy": "node build.js -b && node build.js -d=dist/webclgl/WebCLGL.min.js",
    "build": "browserify src/webclgl/index.js -o dist/webclgl/bundle.js",
    "deploy": "uglifyjs dist/webclgl/bundle.js -cmo dist/webclgl/WebCLGL.min.js"
  },
  "license": "MIT"
}
